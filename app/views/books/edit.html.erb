<h3>Editing Book</h3>
<%= form_with model: @books, url: update_book_path(@books.id), method: :patch do |f| %>
<h4>Title</h4>
<%= f.text_field :title %>
<h4>Boby</h4>
<%= f.text_area :body %>
<%= f.submit 'Update Book' %>
<span><%= link_to 'show', book_path(@books) %></span>
<span><%= link_to 'back', books_path %></span>
<% end %>